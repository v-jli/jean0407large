<?xml version="1.0" encoding="utf-8" ?>
<grid>
  <actions parameters="ObjectID">
    <action name="edit" caption="$General.Edit$" icon="Edit.png" onclick="if (window.EditWebPart) {window.EditWebPart({0}); return false;}" />
    <action name="delete" caption="$General.Delete$" icon="Delete.png" confirmation="$General.ConfirmDelete$" />
    <action name="clone" caption="$unigrid.widget.actions.clone$" icon="Clone.png" />
  </actions>
  <columns>
    <column source="DisplayName" caption="$general.displayname$" wrap="false" localize="true">
      <filter type="text" />
    </column>
    <column width="100%" />
  </columns>
  <options>
    <key name="DisplayFilter" value="true" />
  </options>
  <objecttype name="cms.webpartlist" />
</grid>
